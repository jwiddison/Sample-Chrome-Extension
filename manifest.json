{
  "manifest_version": 2,

  "name": "Seth Quotes",
  "description": "This extension shows an inspirational quote from Seth",
  "version": "0.0.1",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://sethquotesapi.herokuapp.com/quotes",
    "https://code.jquery.com/jquery-3.2.1.min.js",
    "<all_urls>"
  ]
}
